{
  "name": "img-generator",
  "version": "0.1.0",
  "description": "OG image generator core library",
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./client": {
      "import": "./lib/esm/client.js",
      "require": "./lib/cjs/client.js",
      "types": "./lib/types/client.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs && pnpm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rm -rf lib",
    "prebuild": "pnpm run clean",
    "start": "echo 'start'",
    "test": "echo 'test'",
    "dev": "echo 'dev'",
    "publish:npm": "npm publish --access public"
  },
  "dependencies": {
    "satori": "^0.12.0",
    "@resvg/resvg-js": "^2.6.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "keywords": [
    "og-image",
    "image-generator",
    "jsx",
    "react",
    "satori",
    "svg",
    "png"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/img-generator.git"
  }
}
